{
  "testRegex": "((\\.|/*.)(test))\\.tsx?$",
  "verbose": true,
  "moduleNameMapper": {
    "^.+\\.(jpg|jpeg)$": "jest-static-stubs/jpg",
    "^.+\\.gif$": "jest-static-stubs/gif",
    "^.+\\.(eot|otf|svg|ttf|woff|woff2|mp3|m4a|aac|oga|css|less|scss|sass)$": "identity-obj-proxy",
    "^.+\\.svg$": "jest-svg-transformer"
  },
  "globals": {
    "crypto": {
      "getRandomValues": "(arr) => require('crypto').randomBytes(arr.length)"
    }
  }
}
